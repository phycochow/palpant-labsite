server {
    listen 80;
    server_name 13.229.69.154;

    location / {
        proxy_pass http://127.0.0.1:8000;
        include proxy_params;
        proxy_redirect off;
    }

    location /static {
        alias /home/ubuntu/palpant-labsite/static;
    }
}

